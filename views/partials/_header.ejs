<header>
  <nav class="navbar navbar-expand-md navbar-dark" style="background-color:hsl(29, 100%, 50%);font-size: larger; ">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav ">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        
      </div>
    </div>
    <div class="collapse navbar-collapse"  style="display:contents!important;" id="navbarNavAltMarkup" style="float:right;">
      <div class="navbar-nav" >
    <% if (!user) { %>
      <div><a href="/register">
      <button  type = "submit" style="color:white;background-color:#2fc509;border: 1px solid rgb(68, 0, 255); border-radius: 7px;border-color: rgb(77, 208, 20); font-size:medium; padding:8px">Register</button></a></div>
      <div><a href="/login">
        <button  type = "submit" style="color:white;background-color:#2fc509;border: 1px solid rgb(68, 0, 255); border-radius: 7px;border-color: rgb(77, 208, 20); font-size:medium; padding:8px;margin-left: 10px;">Login</button></a></div>
  
    <% } else { %>
   
      Logged in as <%= user.email %>
      <form method = "POST" action="/logout" >
          <button  type = "submit" style="color:white;background-color:#2fc509;border: 1px solid rgb(68, 0, 255); border-radius: 7px;border-color: rgb(77, 208, 20); font-size:medium; padding:8px">Logout</button>
         </form>
  
    <% } %>
  </div>
</div>
  </nav>
</header>